//就地逆置 
#define maxlen 50
#define DATATYPE int
#include<stdio.h>
typedef struct
 {
    DATATYPE arr[maxlen];
    int length;
 }Seqlist;
void print_SQ(Seqlist list)
 {
    for(int i=0;i<list.length;i++)
  {
        printf("%d",list.arr[i]);
      }
    printf("\n");
 }
int main(void)
 {
    Seqlist list = {{1,2,3,4,5,6,7,8,9},9};

    int tmp;
    for(int i=0,j=list.length-1;i<j;i++,j--)//list.arr[i]与list.arr[j]进行交换
 {
        
        tmp = list.arr[i];
        list.arr[i] = list.arr[j];
        list.arr[j] = tmp;
     }

    print_SQ(list);
 }





//二分插入 
void BinInsertSort(int A[], int n)
{
    int i, j, low, mid, high, temp;
    for(i = 1; i <= n-1; i++)
    {
        temp = A[i];                    // 辅助变量temp用来保存待排序的元素
        low = 0;
        high = i-1;                     // 有序区间为[0,i-1],且元素按递增排序 
        while(low <= high)              // 二分法插入 
        {
            mid = (low + high)/2;
            if(A[mid] > temp)
                high = mid - 1;
            else
                low = mid + 1;
        }
        for(j = i-1; j >= high+1; j--)  // 将大于temp的元素依次后移 
            A[j+1] = A[j];
        A[high+1] = temp;               // 将待排序的元素插入
    }
}
